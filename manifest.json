{

    "description": "Download Youtube Video",
    "manifest_version": 2,
    "name": "Test",
    "version": "1.0",
    "icons": {
      "48": "icons/icon1.png"
    },
  
    "permissions": [
      "activeTab","downloads"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
    "browser_style": true,
      "default_icon": "icons/icon1.png",
      "default_title": "YT",
      "default_popup": "popup/Donwload.html"
    }
}
